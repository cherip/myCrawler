<script type="text/javascript">
  function DFloader(){
  }
  function DFfetchMMDD(str) {
      var typ = typeof str;
      if (typ == "string") {
          if (/[^0-9]/.test(str)) {
              return [str.slice(5, 7), str.slice(8, 10)];
          }
          str = Number(str);
          if (str < 10000000000) {
              str = str * 1000;
          }
          var n=str;
          str = new Date();
          str.setTime(n);
      } else if (typ == "number") {
          if (str <= 10000000000) {
              str = str * 1000;
          }
          var n=str;
          str = new Date();
          str.setTime(n);
      }
      var mm = str.getMonth() + 1;
      mm = mm < 10 ? "0" + mm : mm + "";
      var dd = str.getDate();
      dd = dd < 10 ? "0" + dd : dd + "";
      return [mm, dd];
  }
  DFDAT['车易购国产车id']='inland';
  DFDAT['车易购进口车id']='overseas';
  DFDAT['车易购Keys']=['dist_id','car_id','dist_name','time','price','brand','subbrand','active_price'];

  var DFTPL={};
  DFTPL[DFDAT['车易购国产车id']]=DFTPL[DFDAT['车易购进口车id']]=function(i,dist_id,car_id,dist_name,time1,price,brand,subbrand,active_price){var html='<tr'+(i%2?' class="tr"':'')+'>'+
'<td width="82" height="20"><a href="http://dealer.auto.sc.sina.com.cn/auto/4s/buycar.php?dist_id='+dist_id+'&car_id='+car_id+'" target="_blank" title="'+brand+'">'+brand.slice(0,6)+'</a></td>'+
'<td width="62"><a href="http://dealer.auto.sc.sina.com.cn/auto/4s/buycar.php?dist_id='+dist_id+'&car_id='+car_id+'" target="_blank" title="'+subbrand+'">'+subbrand.slice(0,6)+'</a></td>'+
'<td width="60">'+active_price+'万</td>'+
'<td><a href="http://dealer.auto.sc.sina.com.cn/auto/4s/index.php?dist_id='+dist_id+'" target="_blank" title="'+dist_name+'">'+dist_name.slice(0,6)+'</a></td></tr>';
return html;
};
  function DFfetchDatToElm(id,dats,keys,max,write){
    var elm=document.getElementById(id),args,vals,html='';
    if(!dats || !dats.length || !write && !elm ) return;
    for(var i=0;i<max && i<dats.length;i++){
      vals=[];
      for(var j=0;j<keys.length;j++)
        vals.push(dats[i][keys[j]]);
      args=[i].concat(vals);
      html+=DFTPL[id].apply(this,args);
    }
    if(write)
      document.write(html);
    else
      elm.innerHTML=html;
  }
    function showTabyh(name,num,id){
      for(i=1;i<(num+1);i++){
        document.getElementById(name+"t"+i).className="";
        document.getElementById(name+i).style.display="none";
      }
      document.getElementById(name+"t"+id).className="tab1";
      document.getElementById(name+id).style.display="block";
    }
</script>
