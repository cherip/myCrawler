    <script type="text/javascript">
    function hnz_LoginSelectClickOther(e){
      var thisObj = e.target?e.target:event.srcElement,tn=thisObj.tagName,text=thisObj.innerHTML,href=thisObj.href;
      do{
        if(thisObj.tagName == "BODY"){break;};
        if(thisObj.id == "hnz_loginFldselect"){
          if(tn=='A'){
            hnz_sina.$("hnz_loginFldselectdis").innerHTML = text;
            document.hnz_loginForm.entry.value=href;
            var hnz_UserInfo=sinaSSOController.getSinaCookie();
            if(hnz_UserInfo){return true;
              //if(href!="http://henan.sina.com.cn/") window.open(href);              
            }else{
              var username = document.hnz_loginForm.username.value;
              var password = document.hnz_loginForm.password.value;
              if(username.length < 1 || username.length > 64)
                break;
              if(password.length < 1 || password.length > 30)
                break;
              sinaSSOController.login(username,password);
            }
            break;
          }
          break;
        }
      }while(thisObj = thisObj.parentNode);
      hnz_LoginSelectClose();
      return true;
    };
    function hnz_clickLoginSelect(){
      if(hnz_sina.$("hnz_loginFldselectop").style.display == "block"){
        hnz_LoginSelectClose();
      }else{
        hnz_sina.$("hnz_loginFldselectop").style.display = "block";
        hnz_sina.addEvent(document.body,"mousedown",hnz_LoginSelectClickOther);
      }
    };
    function hnz_LoginSelectClose(){
      hnz_sina.$("hnz_loginFldselectop").style.display = "none";
      hnz_sina.delEvent(document.body,"mousedown",hnz_LoginSelectClickOther);
      return false;
    };
    function hnz_LoginSelectClick(a){
      hnz_sina.$("hnz_loginFldselectop").style.display = "none";
      var hnz_UserInfo=sinaSSOController.getSinaCookie();
      return !!hnz_UserInfo;
    }
  </script>
