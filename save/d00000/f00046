<script type="text/javascript">
    function hnz_SetHome(aTag){
     // var href=aTag && aTag.href?aTag.href:window.lo<strong></strong>cation.href;
      var href="http://sc.sina.com.cn";
      try{
        document.body.style.behavior='url(#default#homepage)';
        document.body.setHomePage(href);
      }catch(e){
        if(window.netscape){
          try{
            netscape.security.PrivilegeManager.enablePrivilege("UniversalXPConnect");
          }catch(e){
            alert("抱歉，此操作被浏览器拒绝！\n\n请在浏览器地址栏输入'about:config'并回车然后将[signed.applets.codebase_principal_support]设置为'true'");
          };
        }else{
          alert("抱歉，您所使用的浏览器无法完成此操作。\n\n您需要手动将'http://sc.sina.com.cn/'设置为首页。");
        };
      };
    };
    function hnz_AddFavorite(aTag,sTitle){
      var href=aTag && aTag.href?aTag.href:window.location.href;
      sTitle=sTitle || document.title;
      var href="http://sc.sina.com.cn";
      sTitle="新浪四川";
      try{
        if(window.sidebar && typeof window.sidebar.addPanel=="function")
          window.sidebar.addPanel(sTitle, href, "");
        else if(window.external){
          window.external.addFavorite(href,sTitle);
       }
        else
          alert("加入收藏失败，请使用Ctrl+D进行添加");
      }catch (e){
        alert("加入收藏失败，请使用Ctrl+D进行添加");
      }
    };
  </script>
