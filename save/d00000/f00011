<script type="text/javascript">
function g(o){
	return document.getElementById(o);
}
function sch(){
	var keywords=g("search").value;
	keywords= encodeURI(keywords);
	var go_href = "http://www.google.cn/search?q="+keywords;
	go_href = go_href +"%20site%3Ahaibao.cn"
	window.open(go_href);
	return false;
}
function bookmark(){
	var url="http://www.haibao.cn";
	var title="海报网_时尚_快乐_新鲜_自我_City Girl的互动媒体！";
	if(document.all)
		window.external.AddFavorite(url, title);
	else if (window.sidebar)
		window.sidebar.addPanel(title, url, "")
}
function setHome(a){
	a.style.behavior="url(#default#homepage)";
	a.setHomePage("http://www.haibao.cn");
}
function regHover(a){
	a.style.backgroundPosition="0px -20px";
}
function regOut(a){
	a.style.backgroundPosition="0px 0px";
}

var q_area;
function insertSmiley(smilieid) {
	var src = $('#smilie_' + smilieid)[0].alt;
	bc_tag(q_area[0], {openTag:' ' + src + ' ', closeTag:''});
}
function insertSmiley2(oE) {
	var src = oE.alt;
	bc_tag(q_area[0], {openTag:' ' + src + ' ', closeTag:''});
}
function qsmile(p) {
	$('#qsmiles')[0].innerHTML = strs[p-1];
	$(".facialpage a").removeClass("hoversmile");
	$("#qsmilep"+p).addClass("hoversmile");
}
function fnQsmilesC(e) {
	var evt = e || window.event;
	var oE = evt.target || evt.srcElement;
	if(oE.tagName == 'IMG') {
		bc_tag(q_area[0], {openTag:' ' + oE.alt + ' ', closeTag:''});
	}
}document.getElementById('qsmiles').onclick = fnQsmilesC;
$(document).ready( function() {
	$("#id_thread_url").val( window.location.href );
	$(window).unload(function(){
		if ($(".bbcodeEditor-editor").length && $(".bbcodeEditor-editor").val().length > 0){
			save_data($("#id_title").addClass("autosave")[0], "!title!", $("#id_title").val());
			save_data($(".bbcodeEditor-editor").addClass("autosave")[0], "!content!", $(".bbcodeEditor-editor").val());
		}
	});
	if ( ($.browser.msie && $.browser.version<=5.0) || ($.browser.mozilla && firefox_ver<2) ) {
		$('#restoredata').toggle();
	} else {		
		$('#restoredata').click(function(){
			if(!confirm("此操作将覆盖当前帖子内容，确定要恢复数据吗？")) return;
			try{
				$('#id_title').val(load_data($('#id_title').addClass("autosave")[0], "!title!"));
				$(".bbcodeEditor-editor").val(load_data($(".bbcodeEditor-editor").addClass("autosave")[0], "!content!").toString());
			} catch(e) {}			
		});
	}	
		
});

function get_thread_id() { return [256029]; }
