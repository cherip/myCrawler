<script type="text/javascript">
	function copyto(text, alertmsg){
	text = window.location.href;
	if($.browser.msie) {
	clipboardData.setData('Text', text);
	alert(alertmsg);
	} else {
	prompt('请按下 "Ctrl+C" 来复制本帖地址', text);
	}
	return false;
	}
</script>

