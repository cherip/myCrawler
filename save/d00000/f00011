GA_googleFetchAds();
function fnMoveGoogleAdTo(sGA,oE) {
	if(navigator.AppName.indexOf('IE') > -1) {var b = true;}
	if(!$_(sGA) || !$_(sGA).getElementsByTagName('iframe')[0]) {return false;}
	else{
		var oI = $_(sGA).getElementsByTagName('iframe')[0].contentWindow.document;
		if(!b) {var sd = oI.lastChild.innerHTML;}
		oE.appendChild($_(sGA));
		if(!b) {
			oI = $_(sGA).getElementsByTagName('iframe')[0].contentWindow.document;
			oI.open();
			oI.write('<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><html xmlns="http://www.w3.org/1999/xhtml">'+sd+'</html>');
			oI.close();
		}
		return true;
	}
}
</script>
