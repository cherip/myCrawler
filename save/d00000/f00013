					var arr = [];
					for(var i=0;i<4;i++){
						var num = Math.floor(Math.random()*list_z.length);			
						arr[i] = list_z[num];
						list_z.splice(num,1);
					}
					var str="";
					for(var i=0;i<4;i++){
						arr[i].picurl = (arr[i].picurl).substr(0,arr[i].pic_len);
						str += "<li>";
						str += "	<a href="+arr[i].url+" title="+arr[i].alltitle+"><img width=140 src="+arr[i].picurl+" ></a>";
						str += "	<p>";
