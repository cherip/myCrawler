<script type="text/javascript" language="javascript">
	function toggle()
	{
		var togg = document.getElementById('list_search_selects');
		with(togg.style)
		{
		  if(display != "block")
		  {
			display = "block";
		  }
		  else
		  {
			display = "none";
		  }
		}
		var li =  togg.getElementsByTagName('li');
		for(var i=0;i<li.length;i++)
		{
			li[i].onclick = function(){
				var isie=document.all?true:false
				var _text = '';
				if(!isie) _text = this.textContent;
				else _text = this.innerText;
				document.getElementById('search_selects').innerHTML = _text;
				 togg.style.display = "none";
				 document.getElementById('type').value = this.getElementsByTagName('input')[0].value;
			}                     
		}
	}
</script>

