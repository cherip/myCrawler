<SCRIPT language=javascript>
function changeTab(pid,tabIndex,num)
{
	for (i = 1; i <= num; i++)
	{
		var tab = document.getElementById(pid+ "Tab" +i);
		var content = document.getElementById(pid+ "Content" + i);
		if (i == tabIndex)
			{content.style.display = ""; }
		else
			{content.style.display = "none";}
	}
}
</SCRIPT>
