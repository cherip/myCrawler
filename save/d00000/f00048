                   <script type="text/javascript">
function runsearchkeyword()
{
var website;
var keyword;
var websiteUrl;
website = document.getElementById("searchkeywordsite").options[document.getElementById("searchkeywordsite").selectedIndex].value  ;
keyword = document.getElementById("searchkeywordtext").value ;
if ( website == "itnews" )
{
	websiteUrl = "/search_result.asp?At=News&keyword="+ keyword +"";
}
else if ( website == "ittime" )
{
	websiteUrl = "/search_result.asp?keyword="+ keyword +"";
}
else if ( website == "google" )
{
	websiteUrl = "http://www.google.cn/search?q="+ encodeURI( keyword ) +"&hl=zh-CN&lr=lang_zh-CN&ie=UTF-8&oe=UTF-8";
}
else if ( website == "baidu" )
{
	websiteUrl = "http://www.baidu.com/s?wd="+ keyword +"&cl=3";
}
else if ( website == "yahoo" )
{
	websiteUrl = "http://www.yahoo.cn/s?p="+ encodeURI( keyword ) +"&v=web";
}
else if ( website == "sogou" )
{
	websiteUrl = "http://www.sogou.com/web?query="+ escape( keyword ) +"";
}
else
{
	return false;
}
//alert( websiteUrl );
//return false;
window.open( websiteUrl, website );
return false;
}
                </script>
