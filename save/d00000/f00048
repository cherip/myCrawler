<script type="text/javascript">
function OnLineService_qq(){
      var yMenuFrom_qq, yMenuTo_qq, yOffset_qq, timeoutNextCheck_qq;
      yMenuFrom_qq   = parseInt (document.getElementById("divMenu_qq").style.top, 10);
      yMenuTo_qq     = document.documentElement.scrollTop + 135; //距页面顶部的距离
      timeoutNextCheck_qq = 400;
      if ( yMenuFrom_qq != yMenuTo_qq ) {
              yOffset_qq = Math.ceil( Math.abs( yMenuTo_qq - yMenuFrom_qq ) / 20 );
              if ( yMenuTo_qq < yMenuFrom_qq )
                      yOffset_qq = -yOffset_qq;
              document.getElementById("divMenu_qq").style.top = parseInt (document.getElementById("divMenu_qq").style.top, 10) + yOffset_qq+"px";//加"px"以兼容于火狐
              timeoutNextCheck_qq = 10;
      }
      setTimeout ("OnLineService_qq()", timeoutNextCheck_qq);
}
function CloseQQ()
{
 document.getElementById("divMenu_qq").style.visibility = "hidden";
}
OnLineService_qq();
</script>
<!--End: 在线客服 -->
</body>
</html>
