		var currentPosition = 1;
		
		YAHOO.util.Event.on('body', 'load', function() {
			var fromVal = (currentPosition-1)*(-175);
			currentPosition++;
			var toVal = (currentPosition-1)*(-175);
			var attributes = { left: { from: fromVal, to: toVal, unit: 'px' }};
			var myAnim = new YAHOO.util.Anim('tab_scoot', attributes, 0.5, YAHOO.util.Easing.easeBoth);
			myAnim.animate();
		});	
	
		YAHOO.util.Event.on('scrollLeft', 'click', function() {
			var fromVal = (currentPosition-1)*(-175);
			currentPosition++;
			var toVal = (currentPosition-1)*(-175);
			var attributes = { left: { from: fromVal, to: toVal, unit: 'px' }};
			var myAnim = new YAHOO.util.Anim('tab_scoot', attributes, 0.5, YAHOO.util.Easing.easeBoth);
			myAnim.animate();
		});		
		
		YAHOO.util.Event.on('scrollRight', 'click', function() {
		   if(currentPosition > 1){
				var fromVal = (currentPosition-1)*(-175);
				currentPosition--;
				var toVal = (currentPosition-1)*(-175);
				var attributes = {left: { from: fromVal, to: toVal, unit: 'px' }};
				var myAnim = new YAHOO.util.Anim('tab_scoot', attributes, .5, YAHOO.util.Easing.easeBoth);
				myAnim.animate();
			}
		});		

		</script>
