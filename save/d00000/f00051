function chageItem(n)
{
   document.getElementById("itemBar").className = "itemBar"+ n
   for(i=1;i<=2;i++)
   {
       document.getElementById("c"+i).className = "hide"
   }
   document.getElementById("c"+n).className = "show"

}
</script>

