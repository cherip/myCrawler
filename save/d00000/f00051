	var bid = function(id)
	{
		return document.getElementById(id);
	}
	var show_hot_news = function(k)
	{
		var re = false;
		for(i=0;i<2;i++)
		{
			if(i==k)
			{
				if(bid('news_c_'+i).className == 'here' && (i!=0)) re = true;
				else bid('news_c_'+i).className='here';
				bid('news_t_'+i).style.display='block';
			}else{
				bid('news_c_'+i).className='';
				bid('news_t_'+i).style.display='none';
			}
		}
		return re;
	}
