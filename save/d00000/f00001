			var up_category_ul = $I('#category_childrenall');
			var up_category_div = $I('#category_parentall');
			function show_hide_category(self, alias) {
				var ulo = $I('#category_children' + alias);
				if(!self) {
					ulo = $C('.' + alias)[0].parentNode.parentNode;
					self = $I('#category_parent' + PF.dom.getAttr(ulo, "rel"));
				}
				if(up_category_ul) {
					$_setStyle(up_category_ul, 'display', 'none');
					$_setStyle(up_category_div, 'display', 'block');
				}
				if(ulo) {
					$_setStyle(self, 'display', 'none');
					$_setStyle(ulo, 'display', 'block');
					up_category_ul = ulo;
					up_category_div = self;
				}
			}
					</script>
