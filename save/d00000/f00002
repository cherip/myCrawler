<script type="text/javascript" charset="utf-8">
	var passwd_show 	= $I('#passwd_show');
	var user_passwd 	= $I('#user_passwd');
	var login_name 	= $I('#login_name');
	var delete_name = $I('#delete');
	var cookie_name = "";
	$_addHandler(passwd_show, 'focus', function(){
		$_setStyle(passwd_show,'display', 'none');
		$_setStyle(user_passwd,'display', 'block');
		$_setStyle(user_passwd, 'color', 'black');
		user_passwd.focus();
		if(login_name.value != cookie_name){
			user_passwd.value = '';
		}			
	});
	$_addHandler(user_passwd, 'click', function(){
		$_setStyle(user_passwd, 'color', 'black');
		if(login_name.value != cookie_name){
			user_passwd.value = '';
		}	
	});
	$_addHandler(user_passwd, 'blur', function(){
		var value = user_passwd.value;
		if(value == ''){
			$_setStyle(user_passwd,'display', 'none');
			$_setStyle(passwd_show,'display', 'block');
		}else{
			$_setStyle(user_passwd, 'color', '');
		}
	});
	if(login_name.value == '登录名/邮箱') {
		if(cookie_name != '') {
			login_name.value = cookie_name;
		}
	}
	$_addHandler(login_name, 'click', function(){
		if(cookie_name != '') {
			login_name.select();
		}
		$_setStyle(login_name, 'color', 'black');
		if(login_name.value != ''){
			$_setStyle(delete_name,'display', 'block');
		}
	});
	$_addHandler(login_name, 'blur', function(){
		var value = login_name.value;
		$_setStyle(delete_name,'display', 'block');
		$_setStyle(login_name, 'color', '');
		if(value == '' || value == null){
			login_name.value = '登录名/邮箱';
			$_setStyle(delete_name,'display', 'none');
		}
	});
	$_addHandler(login_name,'keyup',function(){
		if(login_name.value == ''){
			$_setStyle(delete_name,'display', 'none');
		}else{
			$_setStyle(delete_name,'display', 'block');
		}
	});
	$_addHandler(delete_name, 'click', function(){
		login_name.value = '';
		$_setStyle(delete_name,'display', 'none');
		login_name.focus();
	});
</script>
