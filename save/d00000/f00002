				<script type="text/javascript">
function bindEvent(nodes) {
	
	nodes = nodes || null;
	var items = [],
		reviewbtns = [],
		commentforms = [],
		inputs = [],
		sends = [],
		likes = [];
	if (nodes) {
		items = nodes;
		for (var i = 0, j = nodes.length; i < j; i++) {
			!!$C(".reviewbtn", nodes[i])[0] && reviewbtns.push($C(".reviewbtn", nodes[i])[0]);
			!!$C(".comments", nodes[i])[0] && commentforms.push($C(".comments", nodes[i])[0]);
			!!$T("textarea", nodes[i])[0] && inputs.push($T("textarea", nodes[i])[0]);
			!!$C(".send", nodes[i])[0] && sends.push($C(".send", nodes[i])[0]);
			!!$C(".fwlike", nodes[i])[0] && likes.push($C(".fwlike", nodes[i])[0]);
		}
	} else {
		items = $(".pin");
		reviewbtns = $(".pin .reviewbtn");
		commentforms = $(".pin .comments");
		inputs = $(".pin textarea");
		sends = $(".pin .send");
		likes = $(".pin .fwlike");
	}
	
	// 滑入滑出
	for (var i = 0, j = items.length; i < j; i++) {
		(function(i) {
			var that = items[i];
			$_addHandler(that, "mouseover", function() {
				$_addClass(that, "current");
				if((!PF.browser.is360se && !PF.browser.ie) && $_getStyle(that, 'opacity') >= 1) {
					$_setStyle(that, "opacity", "0.9");
				}
			});
			$_addHandler(that, "mouseout", function() {
				$_removeClass(that, "current");
				if((!PF.browser.is360se && !PF.browser.ie) && $_getStyle(that, 'opacity') == 0.9) {
					$_setStyle(that, "opacity", "1");
				}
			});
		})(i);		
	}
	for (i = 0, j = reviewbtns.length; i < j; i++) {
		(function(i) {
			var that = reviewbtns[i],
				forms = $_children(commentforms[i]),
				form = forms[forms.length - 1];
			$_addHandler(that, "click", function(event) {
				pf.event.preventDefault(event);
				if ($_getStyle(form, "display") == "none") {
					$_setStyle(form, "display", "block");
				}
				$_children($_children(form)[1])[0].focus();
				layout.reload(function(){});
			});
		})(i);
	}
	for (i = 0, j = likes.length; i < j; i++) {
		(function(i) {
			var self = likes[i];	
			$_addHandler(self, "click", function (event) {
				var fw_url = PF.dom.getAttr(self, "href");
				var selfHtml = self.innerHTML;
				$_ajaxXmlHeader(fw_url, {
					onsuccess: function (data) {
						var fw_id = parseInt(data);
						if(fw_id > 0){
							var butText = $_children(self);
							var className = PF.dom.getAttr(butText[0],"className");
							if(!className)
								className = PF.dom.getAttr(butText[0],"class");
							if(className == '' || className == null){
								//$_removeClass(butText[0], "PF_icon11");
								//$_addClass(butText[0], "PF_icon12");
								$_addClass(butText[0], "PF_like");
								$_removeClass(self, "o_left01_r");
								$_addClass(self, "o_left01");
								butText[0].innerHTML = "喜欢"
							}else{
								//$_removeClass(butText[0], "PF_icon12");
								//$_addClass(butText[0], "PF_icon11");
								$_removeClass(butText[0], "PF_like");
								$_removeClass(self, "o_left01");
								$_addClass(self, "o_left01_r");
								butText[0].innerHTML = "取消"
							}
						}else{
							alert("操作失败, 请重新操作");
						}
					}
				});
				PF.event.preventDefault(event);
			});
		})(i);
	}
	for (i = 0, j = inputs.length; i < j; i++) {
		(function(i) {
			var that = inputs[i];
			$_addHandler(that, "input", function() {
				$_setStyle(that, "height", (that.scrollHeight + "px"));
				layout.reload(function(){});
			});
			$_addHandler(that, "propertychange", function() {
				$_setStyle(that, "height", (that.scrollHeight + "px"));
				layout.reload(function(){});
			});
		})(i);
	}
	for (i = 0; i < sends.length; i++) {
		(function(){
			var that = sends[i],
				sendStart = false;
			$_addHandler(that, "click", function(event) {
				pf.event.preventDefault(event); // 取消默认行为
				pf.event.stopPropagation(event); // 防止冒泡
				var parent = pf.dom.getParent(that),
					comment = pf.dom.getParent(pf.dom.getParent(parent)),
					action = parent.action,
					textarea = $_children(parent)[0],
					value = pf.string.trimStart(textarea.value);
				if (value.length <= 0) {
					alert("评论内容不能为空！");
					textarea.focus();
				} else { // 提交数据到后台，并等待回应，获得回应后插入数据；
					!sendStart && (function() {
						sendStart = true;
						$_ajax(action, {
							data: "comment_body=" + value, // 数据
							method: "POST", // 用post方法提交
							onsuccess: function(data) { // 成功响应
								//alert(data);
								var div = $_create("div", {id: "tempData"});
								div.innerHTML = data;
								parent.appendChild(div);
								var tempChildren = $_children($I("#tempData"))[0];
								$_remove(div);
								$_insertBefore(tempChildren, $_children(comment)[0]);
								textarea.value = "";
								layout.reload(function(){});
								sendStart = false;	
							}
						});
					})();
				};
			});	
		})(i);
	}
}
bindEvent();
var ajaxStart = false;
var ajaxRequest = function() {
	if (pf.bom.isPageBottom() && !ajaxStart) {
		var elements = [],
		container = layout ? layout._container : null,
		nextpage = $I("#next"),
		nexturl = nextpage ? nextpage.href : "#",
		loading = $I("#loadingtip");
		ajaxStart = true;
		
		$_setStyle(loading, "visibility", "visible");
		
		$_ajaxXmlHeader(nexturl,{
			onsuccess: function(data) {
				$_setStyle(loading, "visibility", "hidden");
				$_remove(nextpage); // 移除nextpage节点，因为服务器端会返回新的节点
				var div = $_create("div", {id: "temp"}); // 临时容器
				div.innerHTML = data;
				container.appendChild(div);
				var tempChildren = $_children($I("#temp"));
				$_remove(div);
				for (var i = 0, j = tempChildren.length; i < j; i++) {
					var temp = tempChildren[i];
					container.appendChild(temp);
					if (i < (j -1)) {
						elements.push(temp);
					}
				}
				div = null;
				ajaxStart = false;
				if (elements) {
					layout.append(elements);
					bindEvent(elements);
				} else {
					$_removeHandler(window, "scroll", ajaxRequest);
				}
				if (elements.length < 25) {
					//ajaxStart = true;
					$_removeHandler(window, "scroll", ajaxRequest);
				}
				if(pf.bom.isPageBottom() && tempChildren.length > 1)
					ajaxRequest();
			}
		});
	}
};
$_addHandler(window, "scroll", ajaxRequest);
$_addHandler(window, "scroll", function() {
    if (pf.bom.getScrollTop() > 0) {
        $_setStyle($I("#top_button"), "display", "block");
    } else {
        $_setStyle($I("#top_button"), "display", "none");
    }
});
$_addHandler($I('#top_button'), "click", function() {
	goTop(2, 20);
	return false;
});
function goTop(acceleration, time) {

	window.onscroll = function(){
		return false;
	}
	
	acceleration = acceleration || 0.1;
	time = time || 16;
 
	var x1 = 0;
	var y1 = 0;
	var x2 = 0;
	var y2 = 0;
	var x3 = 0;
	var y3 = 0;
 
	if (document.documentElement) {
		x1 = document.documentElement.scrollLeft || 0;
		y1 = document.documentElement.scrollTop || 0;
	}
	if (document.body) {
		x2 = document.body.scrollLeft || 0;
		y2 = document.body.scrollTop || 0;
	}
	var x3 = window.scrollX || 0;
	var y3 = window.scrollY || 0;
 
	// 滚动条到页面顶部的水平距离
	var x = Math.max(x1, Math.max(x2, x3));
	// 滚动条到页面顶部的垂直距离
	var y = Math.max(y1, Math.max(y2, y3));
 
	// 滚动距离 = 目前距离 / 速度, 因为距离原来越小, 速度是大于 1 的数, 所以滚动距离会越来越小
	var speed = 1 + acceleration;
	window.scrollTo(Math.floor(x / speed), Math.floor(y / speed));
 
	// 如果距离不为零, 继续调用迭代本函数
	if(x > 0 || y > 0) {
		var invokeFunction = "goTop(" + acceleration + ", " + time + ")";
		window.setTimeout(invokeFunction, time);
	}
}

//mention 如果是新浪用户的用户名 改target 为_blank 用来弹出窗口
var changeLinkTarget = function(){
	var m_links = $C(".external");
	for(var i = 0 ; i < m_links.length ; i ++){
		m_links[i].target = '_blank'
	}
}
changeLinkTarget();

function layoutFade() {
	if(this.tagName != 'A'){
		$_fadeIn(this);
		var as = $C('.alpha', this);
		for(var i = 0;i < as.length;i++) {
			if(!PF.browser.is360se && !PF.browser.ie) {
				$_fadeIn(as[i]);
			} else {
				$_removeClass(as[i], 'alpha');
			}
		}
	}
}
</script>
