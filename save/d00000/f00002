	<script src="http://static.pinfun.com/js/pinfun_silder.js"></script>
<script>
var loadImage = function(img) {
	if (!img.getAttribute('_preload')) {
		return;
	}
	img.src = img.getAttribute('_preload');
	img.removeAttribute('_preload');
}
var lis = [],
	timer = null,
	imgs = getTagName('img', 'huandeng'), // 图片预加载处理，首先获得图片数组
	n = $I('#slider-list').getElementsByTagName('li').length;
var slider = new SlideTrans('huandeng', 'slider-list', n, {
	OnStart: function() {
		var i = slider.Index + 1;
		if (i < imgs.length) {
			loadImage(imgs[slider.Index+1]);
		}
		each(lis, function(i) {
			if(PF.dom.getAttr(lis[i], 'rel') - 1 == slider.Index) {
				$_removeClass(lis[i].children[0].children[0], 'pin02');
				$_addClass(lis[i].children[0].children[0], 'pin01');
			} else {
				$_removeClass(lis[i].children[0].children[0], 'pin01');
				$_addClass(lis[i].children[0].children[0], 'pin02');
			}
		});
	},
	Pause: 4000,
	Direction: false,
	Duration: 50
});
for (var i = 0; i < n; i++) {
	(function(i) {
		var li = $I('#slider-number').children[i];
		addEvent(li, 'mouseover', function() {
			timer = setTimeout(function() {
				$_removeClass(li.children[0].children[0], 'pin02');
				$_addClass(li.children[0].children[0], 'pin01');
				loadImage(imgs[i]);
				slider.Auto = false; 
				slider.run(i);									
			}, 100);
		});
		addEvent(li, 'mouseout', function() {
			clearTimeout(timer); 
			$_removeClass(li.children[0].children[0], 'pin01');
			$_addClass(li.children[0].children[0], 'pin02');
			slider.Auto = true; 
			slider.run();
		});
		lis[i] = li;
	})(i);
}
//slider.run();
</script>
