        <!-- START Javascript For scrolling behavior -->
<script type="text/javascript">
//js object that holds scroll information
var scrollanim = {time:0, begin:0, change:0.0, duration:0.0, element:null, timer:null};

function reset(elem)
{
  clearInterval(scrollanim.timer);
  scrollanim.timer = null; 
  elem.scrollTop = 0;
}

function doScroll(elem)
{
  //alert("scrollHeight: " + elem.scrollHeight + "\n" + "offsetHeight: " + elem.offsetHeight)
  if(elem.scrollHeight > 32)
  {
    if (scrollanim.timer != null)
    {
      clearInterval(scrollanim.timer);
      scrollanim.timer = null;
    }

    var finish = 32;
    var start = 0;
    scrollanim.time = 0;
    scrollanim.begin = 0;
    //alert("end:" + end + " start:" +  start);
    scrollanim.change = finish - start;
    scrollanim.duration = 450;
    scrollanim.element = elem;
    scrollanim.timer = setInterval("scrollTopAnim();", 15);
  }
}

function scrollTopAnim()
{
  if (scrollanim.time > scrollanim.duration) 
  {
    clearInterval(scrollanim.timer);
    scrollanim.timer = null;
  }
  else
  {
    move = sineInOut(scrollanim.time, scrollanim.begin, scrollanim.change, scrollanim.duration);
    //alert(move);
    scrollanim.element.scrollTop = move;
    //alert(scrollanim.element.scrollLeft);
    scrollanim.time++;
  }
}
function sineInOut(t, b, c, d) { return c*t/d+b; }
</script>
<!-- End Javascript For scrolling behavior -->



