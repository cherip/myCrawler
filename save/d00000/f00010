<script>
	var nav = 'angie/pins';
	var index = nav.indexOf("/");
	if(index != -1) {
		nav = nav.substring(index + 1, nav.length);
	} else {
		nav = '';
	}
	$_addClass($I('#nav' + nav), 'current');
</script>
