$(document).ready(function() {
	$('ul.sf-menu').superfish({firstOnClick:true,delay:100,speed:'fast'});
	$(".videowrapped").hover(function() {
		var huboffset = $('#hubinfo').offset();
		var offset = $(this).offset();
		var y = offset.top - huboffset.top;
		var x = offset.left - huboffset.left + 140;

		var elem = $(this).next(".hoverinfo");
		if ( elem.hasClass('righthover') )
			x -= 384;
		elem.css('left', x);
		elem.css('top', y);
		elem.show();

	}, function() {
		$(this).next(".hoverinfo").hide();
	});	
});

