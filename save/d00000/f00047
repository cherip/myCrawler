         <script type="text/javascript" language="javascript">
             function divonmouseover(a) {
                 if (a == 1) {
                     document.getElementById("zxhy").style.display = "block";
                     document.getElementById("zxzh").style.display = "none";
                     document.getElementById("zxpx").style.display = "none";
                     document.getElementById("zxhydiv").className = "text";
                     document.getElementById("zxzhdiv").className = "text2";
                     document.getElementById("zxpxdiv").className = "text2";
                 }
                 else if (a == 2) {
                     document.getElementById("zxhy").style.display = "none";
                     document.getElementById("zxzh").style.display = "block";
                     document.getElementById("zxpx").style.display = "none";
                     document.getElementById("zxhydiv").className = "text2";
                     document.getElementById("zxzhdiv").className = "text";
                     document.getElementById("zxpxdiv").className = "text2";
                 }
                 else if (a == 3) {
                     document.getElementById("zxhy").style.display = "none";
                     document.getElementById("zxzh").style.display = "none";
                     document.getElementById("zxpx").style.display = "block";
                     document.getElementById("zxhydiv").className = "text2";
                     document.getElementById("zxzhdiv").className = "text2";
                     document.getElementById("zxpxdiv").className = "text";
                 }

             }
             function hidead() {
                 document.getElementById("divMenu").style.display = "none";
             }
    </script>
     <script type="text/javascript" language="javascript">
     function qk() {
         var txt = document.getElementById('TextBox1').value;
     if (txt == "请输入电子邮箱") {
         document.getElementById('TextBox1').value = "";
     }

 }
 </script>
