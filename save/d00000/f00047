         function Load() {
             var url = "";
             url = top.window.location.href.toLowerCase();
             var str1 = new Array("dy.sino-manager.com",
                "ukp.sino-manager.com",
                "bossway.sino-manager.com",
                "www.gsjr.net",
                "www.sino-manager.com/u/langxianping",
                "www.jlrhd.com",
                "www.jlrhd.net",
                "www.jlrhd.cn",
                "www.活动频道.com",
                "www.woohd.com",
                "www.woohd.net",
                "www.woohd.cn",
                "neikan.sino-manager.com");
             var str2 = new Array("http://www.sino-manager.com/order/order_1.aspx",
              "http://www.sino-manager.com/business/ukp/",
              "http://www.sino-manager.com/bossway/",
              "http://www.sino-manager.com/business/financial/",
              "http://www.sino-manager.com/u/langxianping.html",
              "http://www.jlrhd.com",
              "http://www.jlrhd.net",
              "http://www.jlrhd.cn",
              "http://www.活动频道.com",
              "http://www.woohd.com",
              "http://www.woohd.net",
              "http://www.woohd.cn",
              "http://www.sino-manager.com/business/neikan/");
             for (var i = 0; i < str1.length; i++) {
                 if (url == str1[i].toLowerCase() + "/" || url == "http://" + str1[i].toLowerCase() + "/") {
                     this.location = str2[i].toLowerCase();
                     this.location.search;
                 }
             }
            
//             GetCookie();
//             var id = "1"
             var tmp = GetCookie('key1');
             if (tmp) {
                 var Panel = document.getElementById("Panel1");
                 Panel.style.display = "none";
             }
             else {
                 var id = "1"
//                 var str = "key1=" + escape(id);
//                 var dd = new Date();
//                 dd.setDate(dd.getDate() + 1); //获取明天 
//                 var y = dd.getYear();
//                 var m = dd.getMonth() + 1; //获取当前月份的日期
//                 var d = dd.getDate();
//                 date = new Date(y + '-' + m + '-' + d + ' 00:00:00');
//                 date.setTime(date.getTime()); //明天的0:00:00
//                 str += ";expires=" + date.toGMTString();
//                 alert(str);
                 document.cookie = "key1=" + escape(id);

             }

         }
         
         function GetCookie(Name) {
             var search = Name + "="
             var Cookie = document.cookie;
             if (Cookie.length <= 0 || Cookie.indexOf(search) == -1) return 0;
             else {
                 var tmp = Cookie.split(search)[1].split(";")[0]
                 return unescape(tmp)
             }
         }



         window.onload = Load; 
    </script>

