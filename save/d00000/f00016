var oldactiveid = $(".m1 li a[class='active']").attr('id').replace('pbid_link','');
$(".m1 li a").bind("mouseenter",function(){
	var thisid = $(this).attr('id').replace('pbid_link','');
	$(".m1 li a").removeClass();
	$("#pbid_link19").addClass('zr');
	$(".m2").hide();
	$(this).addClass((thisid==19? 'zr active' : 'active'));
	$("#pbid_ul"+thisid).show();
});
$(".p2").bind("mouseleave",function(){
	var thisid = oldactiveid ? oldactiveid : '1';
	$(".m1 li a").removeClass();
	$("#pbid_link19").addClass('zr');
	$(".m2").hide();
	$("#pbid_link"+thisid).addClass((thisid==19 ? 'zr active':'active'));
	$("#pbid_ul"+thisid).show();
});


