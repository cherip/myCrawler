	function setNotifyNums() {
		$.ajax({
			cache:false,
			url:"/ajax/stat/statcheck.php",
			data:"action=checkCareRows",
			success:function(data){
				$("#notify_btn b.p1b").text(data);
			}
		});			
	}
	
	function _getCares() {
		getCares('care_list',setNotifyNums);
	}
