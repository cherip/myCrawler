function listshow(){
	var pos = parseInt($("div.gocar_Listedpic").attr("pos"));
	var count = parseInt($("div.gocar_Listedpic").attr("count"));
	var show = parseInt($("div.gocar_Listedpic").attr("show"));
	if(pos == 1)
		$("a.goleft").hide();
	else
		$("a.goleft").show();
	if(pos + show - 1 >= count)
		$("a.goright").hide();
	else
		$("a.goright").show();
		
	$("div.gocar_Listedpic ul li").each(function(loopnum){
		if(loopnum+1>=pos && loopnum < pos+show-1)
			$(this).show();
		else
			$(this).hide();
	});
};
$("div.gocar_Listedpic").attr("count", $("div.gocar_Listedpic ul li").length);
$("div.gocar_Listedpic").attr("show", 6);
$("div.gocar_Listedpic").attr("pos", 1);
listshow();
$("a.goleft").click(function(){
	var pos = parseInt($("div.gocar_Listedpic").attr("pos"));
	if(pos > 1){
		pos--;
		$("div.gocar_Listedpic").attr("pos", pos);
		listshow();
	}
});
$("a.goright").click(function(){
	var pos = parseInt($("div.gocar_Listedpic").attr("pos"));
	var count = parseInt($("div.gocar_Listedpic").attr("count"));
	var show = parseInt($("div.gocar_Listedpic").attr("show"));
	if((pos + show - 1) < count) {
		pos++;
		$("div.gocar_Listedpic").attr("pos", pos);
		listshow();
	}
});

//上市新车 js end

//切换分页
var dpage = 'dealer_g1';
var dbtn = 'dealer_b1';
$("#dpager a").click(function () {$('#'+dbtn).removeAttr('class');$(this).attr('class','on');dbtn=this.id;$('#'+dpage).hide();$('#dealer_g'+$(this).text()).show();dpage='dealer_g'+$(this).text();});
</script>
</body>
</html>