	<script type="text/javascript" charset="utf-8">
		var login_name 		= $I('#login_name');
		var passwd_show 	= $I('#passwd_show');  
		var user_passwd 	= $I('#user_passwd');
		var cookie_name = "";
		$_addHandler(login_name, 'focus', function(){
			 var value = login_name.value;
			 if(value == '登录名/邮箱'){
				login_name.value = '';
				$_removeClass(login_name,'PF_textgc');
				$_addClass(login_name,'PF_textgb');
			}else {
				login_name.select();
			}
		});
		$_addHandler(login_name, 'blur', function(){
			 var value = login_name.value;
			 if(value == ''){
				if(cookie_name != '') {
					login_name.value = cookie_name;
				} else {
					login_name.value = '登录名/邮箱';
				}
				$_removeClass(login_name,'PF_textgb');
				$_addClass(login_name,'PF_textgc');
			}
		});
		
		$_addHandler(passwd_show, 'focus', function(){
			$_setStyle(passwd_show,'display', 'none');
			$_setStyle(user_passwd,'display', 'block');
			user_passwd.focus();			
		});
		
		$_addHandler(user_passwd, 'blur', function(){
			var value = user_passwd.value;
			if(value == ''){
				$_setStyle(user_passwd,'display', 'none');
				$_setStyle(passwd_show,'display', 'block');
			}
		});

		
		if(login_name.value == '登录名/邮箱') {
			if(cookie_name != '') {
				login_name.value = cookie_name;
			}
		}
	</script>
	<script type="text/javascript">
var pkBaseURL = (("https:" == document.location.protocol) ? "https://stat.pinfun.com/" : "http://stat.pinfun.com/");
document.write(unescape("%3Cscript src='" + pkBaseURL + "piwik.js' type='text/javascript'%3E%3C/script%3E"));
</script>
<script type="text/javascript">
try {
