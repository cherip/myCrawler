<script>
//dd
function show_menu(state){
	more_div=document.getElementById('more');
	manu_id_div=document.getElementById('manu_id');
	sub_id_div=document.getElementById('sub_id');
	if(!state){
		more_div.style.display='none';
		//ddd
		if(sub_id_div&&manu_id_div){			
			if(!window.XMLHttpRequest){
				manu_id_div.style.display='block';
				sub_id_div.style.display='block';
			}
		}
	}else{
		more_div.style.display='block';
		//dd
		if(sub_id_div&&manu_id_div){
			if(!window.XMLHttpRequest){
				manu_id_div.style.display='none';
				sub_id_div.style.display='none';
			}
		}
	}
}
</script>
