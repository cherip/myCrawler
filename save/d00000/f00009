		<div class="PF_floatl PF_width_r" style="margin-left:10px;">
			<script type="text/javascript">
(function(l){
		var flag = 'p';
	//屏幕分辨率
	var s = {a:1024,b:1280,c:1366,d:1440,e:1680,f:1920,g:2160,h:2400,i:2640,j:2880};
	//初始化页面宽度
	var p = {a:933,b:1170,c:1408,d:1644,e:1889,f:2127,g:2365,h:2603,i:2853};
	//初始化页面宽度,但不包含页面左侧第一列宽度
	var f = {a:697,b:933,c:1172,d:1410,e:1651,f:1891,g:2130,h:2369,i:2617};
	//导航左边距宽度
	var m = {a:221,b:300};
	//屏幕宽度默认值
	var df = p.b;
	//屏幕宽度默认值,但不包含页面左侧第一列宽度
	var ff = f.b;
	//导航左边距默认值
	var bml = m.b;
	//计算宽度
	function count() {
		var w = getCW();
		if(w <= s.a || w <= p.b) {//４列
			df = p.a;
			ff = f.a;
			bml = m.a;
       	} else if((w <= s.b && w >= s.a) || w <= s.c || w < p.c) {//5列
			df = p.b;
			ff = f.b;
			bml = m.b;
        	} else if((w <= s.d && w >= s.c) || w <= s.d || w < p.d) {//6列
			df = p.c;
			ff = f.c;
			bml = m.b;
        	} else if((w <= s.e && w >= s.d) || w <= s.e || w < p.e) {//7列
        		df = p.d;
			ff = f.d;
			bml = m.b;
        	} else if((w <= s.f && w >= s.e) || w <= s.f || w < p.f) {//8列
        		df = p.e;
			ff = f.e;
			bml = m.b;
        	} else if((w <= s.g && w >= s.f) || w <= s.g || w < p.g) {//9列
			df = p.f;
			ff = f.f;
			bml = m.b;
	 	} else if((w <= s.h && w >= s.g) || w <= s.h || w < p.h) {//10列
			df = p.g;
			ff = f.g;
			bml = m.b;
	 	} else if((w <= s.i && w >= s.h) || w <= s.i || w < p.i) {//11列
			df = p.h;
			ff = f.h;
			bml = m.b;
	 	} else {//12列
			df = p.i;
			ff = f.i;
			bml = m.b;
	 	}
	}
	//设置页面整体宽度
	function setW() {
		var bo = $C('.PF_body')[0];
		if(bo != undefined) {
			$_setStyle($C('.PF_body')[0], 'width', df + 'px');
		}
		var w = getCW();
		if(w <= s.a || w <= p.b) {
			$_setStyle($C('.header02')[0], 'width','auto');	
		} else {
			$_setStyle($C('.header02')[0], 'width', df + 'px');
		}
	}
	//设置个人页面（版块，拼图，喜欢，提到）宽度，不包含左侧第一列宽度
	function setBW() {
		$_setStyle($C('.PF_width_r')[0], 'width', ff + 'px');
	}
	//获取浏览器宽度
   	function getCW() {
		return l.body.clientWidth
   	}
	function init() {
		count();
		setW();
        if(flag == 'p') {
	        setBW();
        }
	}
	//当浏览宽度改变时执行
	window.onresize = function(){
        init();
        if(flag != 'o') {
		if(layout) {
	        layout._init();
	        layout.reload();
		}
        }
   	}
   	init();
})(document);
</script>
	
