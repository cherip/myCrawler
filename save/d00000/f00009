<script type="text/javascript">
    var menu = $C(".menu");
    for (var i = 0; i < menu.length; i++) { (function() {
            var that = menu[i];
            $_addHandler(that, "mouseover",
            function() {
                $_addClass(that, "current");
            });
            $_addHandler(that, "mouseout",
            function() {
                $_removeClass(that, "current");
            });
        })(i);
    }
    var menuLi = $(".menu li");
    for (var i = 0; i < menuLi.length; i++) { (function() {
            var that = menuLi[i];
            $_addHandler(that, "mouseover",
            function() {
                $_addClass(that, "hover");
            });
            $_addHandler(that, "mouseout",
            function() {
                $_removeClass(that, "hover");
            });
        })(i);
    }
var searchtext = $C('.searchtext')[0];
$_addHandler(searchtext, "focus", function() {
	if(searchtext.value == '寻拼客')
	searchtext.value='';
});
$_addHandler(searchtext, "blur", function() {
	if(searchtext.value == '')
	searchtext.value='寻拼客';
});
searchtext.value='寻拼客';
</script>

