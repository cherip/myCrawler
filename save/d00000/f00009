		$_addHandler(text, 'blur', function(){
			if(text.value == ''){
				text.value = "发送私信";
			}
			$_setStyle(text, 'color', '');
		});
		$_addHandler(text, 'focus', function(){
			if(text.value == '发送私信'){
				text.value = '';
			}
			$_setStyle(text, 'color', 'black');
			$_setStyle(text, 'height', '39px');
			$_setStyle(p1,'display', 'block');
		});

		$_addHandler(texta, 'click', function(event){
			//alert(texta.href);
			pf.event.preventDefault(event); // 取消默认行为
			pf.event.stopPropagation(event); // 防止冒泡
			if(text.value == '' || text.value == null || text.value == '发送私信'){
				alert('内容不能为空！');
			}else{
				$_ajaxXmlHeader(texta.href, {
					data:'content=' + text.value + '&root=' + '',
					onsuccess: function (data){
						if(data != ''){
							text.value = '发送私信';
						}else{
							alert(texta_int);
						}
					}
				});
			}
		});
