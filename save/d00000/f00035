        <script type="text/javascript">
            var index=1;
            var settime = 5000;

            $(document)	.ready(function(){
                $(".floatdiv").hover(function(){},function(){
                    $(this).hide();
                });
                $(".showdiv").hover(function(){
                    id = $(this).attr("name");
                    $(".floatdiv").hide();
                    $(".pos"+id).show();
                });

                $(".fdlist").hover(function(){
                    name=$(this).attr("name");
                    link = $(this).attr("link");
                    //$(".fidvshow").attr("src",link);
                    $("#fdivimg"+name).attr("src",link);
                },function(){});

                create_cu3flash();
            });
            var a;
            function showlink()
            {
                if(settime>5000)
                {
                    window.clearTimeout(a);
                }
                $(".lktitle").removeClass("lktitle2");
                $(".lt"+index).addClass("lktitle2");
                $(".linklist").hide();
                $(".ll"+index).show();
                index++;
                if(index>''){index=1;}
                a=window.setTimeout("showlink()", settime)
                settime = 5000;
            }
			$(".lktitle").click(function(){
                    index = $(this).attr("name");
                    settime = 10000;
                    showlink();
                    return false;
                });
			showlink();

            function create_cu3flash(){
                var flashvars = {};
                flashvars.xml =  "/public/focus/focus.xml";
                var params = {};
                params.allowscriptaccess = "always";
                params.allownetworking = "all";
                params.wmode = "transparent";
                params.xml = "focus.xml";
                var attributes = {};
                attributes.id = "slider";
                swfobject.embedSWF("http://www.hi-chic.com/public/flash/cu3er.swf", "3dcube", "934", "320", "9", flashvars, params, attributes);
            }

