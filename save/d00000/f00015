var topc = QueryParam("t") == "pc";
function BrowserRedirect(){
var sAgent = navigator.userAgent.toLowerCase();
//var bIsIpad = sAgent.match(/ipad/i) == "ipad";	
var bIsIphoneOs = sAgent.match(/iphone os/i) == "iphone os";
var bIsMidp = sAgent.match(/midp/i) == "midp";
var bIsUc7 = sAgent.match(/rv:1.2.3.4/i) == "rv:1.2.3.4";
var bIsUc = sAgent.match(/ucweb/i) == "ucweb";
var bIsAndroid = sAgent.match(/android/i) == "android";
var bIsCE = sAgent.match(/windows ce/i) == "windows ce";
var bIsWM = sAgent.match(/windows mobile/i) == "windows mobile";
if(bIsIphoneOs || bIsAndroid||bIsMidp||bIsUc7||bIsUc||bIsCE||bIsWM){ 
if(!topc){
window.location.href = "http://m.cheshi.com/";
}
}
}
function QueryParam(name){//获取参数值
var sUrl = window.location.search.substr(1);
var r = sUrl.match(new RegExp("(^|&)" + name + "=([^&]*)(&|$)"));
return (r == null ? null : unescape(r[2]));
}
BrowserRedirect();
</script>
