  <!--[if !IE]>首页搜索框 开始<![endif]-->
  <script type="text/javascript"> 
function send_form_s(event){  
      if(event.keyCode==13) { 
            check_form_s(); 
       return false; 
       }  
} 
function check_form_s(){
var type = $("#tags").val();
var q = $('#q').val();

if(q == undefined || q == '' || q == "请输入查询内容"){
var url_id = 0;
  switch(type){
  case "news":
  url_id = 1;
  break;
  case "car":
  url_id = 2;
  break;
  case "price":
  url_id = 3;
  break;
  case "image":
  url_id = 4;
  break;
  case "video":
  url_id = 6;
  break;
  case "ask":
  url_id = 5;
  break;
  case "seller":
  url_id = 7;
  break;
}
window.open("http://search.cheshi.com/index.php?t="+url_id);
return false;	
}
q = encodeURIComponent(q);
window.open("http://search.cheshi.com/"+type+".php?q="+q);
return false;
}
function search_word(keyword){
var type = $("#tags").val();
if(keyword == undefined || keyword == '' ) return false;
var	q = escape(keyword);
if(type == 'faq'){
window.open("http://faq.cheshi.com/search.php?q="+q);
}else if(type == 'seller'){
window.location.href="http://seller.cheshi.com/index.php?action=seller&sellername="+keyword;
}else{
window.open("http://search.cheshi.com/"+type +".php?q="+q);
}
return false;
}
function switch_style(id,tags){
for(var i=1;i<9;i++){
$("#s_"+i).attr("class","");
}
$("#s_"+id).attr("class","now");
$("#tags").val(tags);
}
</script> 
