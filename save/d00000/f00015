          <li id="s_2" onclick="switch_style(2,'car');" class="now">车型</li>
          <li id="s_3" onclick="switch_style(3,'price');">报价</li>
          <li id="s_4" onclick="switch_style(4,'image');">图片</li>
          <li id="s_5" onclick="switch_style(5,'video');">视频</li>
          <li id="s_6" onclick="switch_style(6,'ask');">问答</li>
          <li id="s_7" onclick="switch_style(7,'seller');">经销商</li>
