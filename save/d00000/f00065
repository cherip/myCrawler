SurveyObj = {
	bType: true,//调查类型（是否是普通调查,true:为普通调查，false:为其他类型调查，默认为true）
	nVotes: new Array(),//各个选项的票数数组
	nPercents: new Array(),//各个选项的百分比数组
	nOptIds: new Array(),//各个选项的ID号比数组
	nSbjId: null,//调查题目ID号
	nItemLen: null,
