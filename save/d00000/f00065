                 <input name="simple_search" type="text" id="simple_search" value="品牌/型号" align="absmiddle" style="width:150px;border:1px solid #D0DCE8; height:16px;color:#8B8C8C;padding:3px 0px 0px 2px;vertical-align:middle" onblur="blurVerify(this)" onfocus="focusVerify(this)"/>
