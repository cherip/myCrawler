<script>
function tab(o,o2,n,o1c,o2c){
			var m_n = document.getElementById(o).getElementsByTagName(o1c);
			var c_n = document.getElementById(o2).getElementsByTagName(o2c);
			//var d_n = document.getElementById("titles").getElementsByTagName("span");
			for(i=0;i<m_n.length;i++){
				 m_n[i].className=i==n?"on":"";
 				 c_n[i].className=i==n?"dis":"undis";
				 //d_n[i].className=i==n?"dis":"undis";
				 }
		}

var Browser = new Object();

Browser.ua = window.navigator.userAgent.toLowerCase();
Browser.ie = /msie/.test(Browser.ua);
Browser.moz = /gecko/.test(Browser.ua);

//JsLoader
var JsLoader = {
	load: function(sUrl, fCallback)
	{
		var _script = document.createElement("script");
		_script.setAttribute("type", "text/javascript");
		_script.setAttribute("src", sUrl);
		document.getElementsByTagName("head")[0].appendChild(_script);

		if (Browser.ie)
		{
			_script.onreadystatechange = function()
			{
				if (this.readyState=="loaded" || this.readyState=="complete")
				{
					fCallback();
				}
			};
		}
		else if (Browser.moz)
		{
			_script.onload = function()
			{
				fCallback();
			};
		}
		else
		{
			fCallback();
		}
	}
};

var $ = function(s)
{
	return (typeof s == "object") ? s: document.getElementById(s);
};



</script>
</head>

