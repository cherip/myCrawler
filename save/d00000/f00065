	sVotesPre: "item",//用来显示投票结果对象的Id前缀
	sPre: "pre",//用来显示投票结果百分比对象的Id前缀
	ie: /msie/.test(window.navigator.userAgent.toLowerCase()),
	moz: /gecko/.test(window.navigator.userAgent.toLowerCase()),
	isloaded: function(obj, fCallback) {
        if (this.ie) {
            obj.onreadystatechange = function() {
                if (this.readyState == 'loaded' || this.readyState == 'complete') {
                    fCallback()
                }
            }
        } else if (this.moz) {
            obj.onload = function() {
                fCallback()
            }
        } else {
            fCallback()
        }
    },
    LoadJs: function(sUrl, fCallback) {
        var _script = document.createElement('script');
        _script.setAttribute('type', 'text/javascript');
        _script.setAttribute('charset', 'gb2312');
        _script.setAttribute('src', sUrl);
        document.getElementsByTagName('head')[0].appendChild(_script);
        this.isloaded(_script, fCallback)
    },
    getObj: function(id) {
        return document.getElementById(id)
    },
    onload: function() {
	     var urlStr = 'http://page.vote.qq.com/api.php?type=result&id=' + this.sSid + '&now=' + Math.random();
         SurveyObj.LoadJs(urlStr,
		   function(){
		    SurveyObj.nItemLen = eval("survey_result_"+SurveyObj.sSid).result.split(";").length;
		    SurveyObj.nSbjId = eval("survey_result_"+SurveyObj.sSid).result.split(";")[0].split("-")[0];
			for (i = 0; i < SurveyObj.nItemLen; i++) {
			  SurveyObj.nVotes[i] = eval("survey_result_"+SurveyObj.sSid).result.split(";")[i].split(":")[1];
			}
			for (i = 0; i < SurveyObj.nItemLen; i++) {
			if (i < (this.nItemLen - 1)) {
                        SurveyObj.nPercents[i] = eval("survey_result_"+SurveyObj.sSid).result.split(";")[i].split(":")[2];
                    } else {
                        SurveyObj.nPercents[i] = eval("survey_result_"+SurveyObj.sSid).result.split(";")[i].split(":")[2].split("-")[0];
                    }
			}
			for (i = 0; i < SurveyObj.nItemLen; i++) {
			if (i == 0) {
                        SurveyObj.nOptIds[i] = eval("survey_result_"+SurveyObj.sSid).result.split(";")[i].split(":")[0].split("-")[1];
                    } else {
                        SurveyObj.nOptIds[i] = eval("survey_result_"+SurveyObj.sSid).result.split(";")[i].split(":")[0];
                    }
			}
			SurveyObj._type();
		   });
		   
    },
	_type: function() {
	   var urlStr = 'http://page.vote.qq.com/survey.php?PjtID=' + this.sSid + '&result=0&fmt=json&rdm='+ Math.random();
		 this.LoadJs(urlStr,
		   function(){
		     if(eval("survey_result_"+SurveyObj.sSid).errcode == 9){ SurveyObj.bType = false;SurveyObj.inits();}
			 else {SurveyObj.bType = true;SurveyObj.inits();}
		   });
	},
	inits: function() {
	     if(this.bType){
		  var _form = document.createElement('form');
            var _hidden = document.createElement('input');
			var _ul ="";
			var _li = "";
            _form.setAttribute('name', SurveyObj.sformId);
            _form.setAttribute('id', SurveyObj.sformId);
            _form.setAttribute('target', 'Ansy_Post');
            _form.setAttribute('action', 'http://input.vote.qq.com/survey.php');
            _form.setAttribute('method', 'post');
            _hidden.setAttribute('name', SurveyObj.sPjtID);
            _hidden.setAttribute('type', 'hidden');
            _hidden.setAttribute('value', SurveyObj.sSid);
			
			for (i = 0; i < this.nItemLen; i++) {
			  _li += '<li></li>'
			}
			_ul = '<ul>'+ _li +'</ul>';
			this.getObj("map_box").innerHTML=_ul;
            _form.appendChild(_hidden);
            if (SurveyObj.ie) {
                for (i = 0; i < this.nItemLen; i++) {
                    var _radio = document.createElement('<input type="radio"   name="sbj_' + SurveyObj.nSbjId + '[]" id="rad' + i + '" value="' + SurveyObj.nOptIds[i] + '" class="undis" />');
                    _form.appendChild(_radio);
                    _radio = null
                }
            } else {
                for (i = 0; i < this.nItemLen; i++) {
                    var _radio = document.createElement('input');
                    _radio.setAttribute('name', 'sbj_' + SurveyObj.nSbjId + '[]');
                    _radio.setAttribute('type', 'radio');
                    _radio.setAttribute('class', 'undis');
                    _radio.setAttribute('id', 'rad' + i);
                    _radio.setAttribute('value', SurveyObj.nOptIds[i]);
                    _form.appendChild(_radio);
                    _radio = null
                }
            }
            document.getElementsByTagName('body')[0].appendChild(_form)
		 }else{
		 var _ul ="";
			var _li = "";
		   for (i = 0; i < this.nItemLen; i++) {
			  _li += '<li></li>'
			}
			_ul = '<ul>'+ _li +'</ul>';
			this.getObj("map_box").innerHTML=_ul;
		 }
		 DomCtrl();
    },
	survey_do_get: function(obj, optId) {
	
	   if(SurveyObj.nNow==0){
        var SubmitUrl = "http://page.vote.qq.com/survey.php?PjtID=" + SurveyObj.sSid;
			SubmitUrl += "&SubjID=";
			SubmitUrl += SurveyObj.nSbjId;
			SubmitUrl += "&OptID=";
			SubmitUrl += optId;
			SubmitUrl += "&fmt=json";
			SubmitUrl += "&result=0";
			SubmitUrl += "&rdm=" + Math.random();
			
			SurveyObj.LoadJs(SubmitUrl,function(){
			  if(eval("survey_result_"+SurveyObj.sSid).errcode == 0){
			    alert("提交成功，感谢参与！");
			    SurveyObj.onload();
			  }
			})
			SurveyObj.nNow = 9;
			setTimeout("SurveyObj.change()",1000);
		 }else{
		   alert("对不起，您已经提交过了！");
		 }
    },
    survey_do_post: function(n) {
        this.getObj(this.sRadioIdPre + n).checked = true;
        this.getObj(this.sformId).submit();
        var iframe = SurveyObj.getObj('Ansy_Post');
		document.getElementsByTagName('body')[0].removeChild(this.getObj(this.sformId));
        this.isloaded(iframe,
        function() {
            SurveyObj.onload();
        })
    },
	change: function() {
        this.nNow--;
		if(this.nNow != 0){setTimeout("SurveyObj.change()",1000);}
    }
}
SurveyObj.onload();
var DomCtrl =function (){//对返回的json数据进行操作，可以根据需要自行修改。
	for (i = 0; i < SurveyObj.nItemLen; i++) {
		SurveyObj.getObj(SurveyObj.sVotesPre + i).innerHTML = SurveyObj.nVotes[i];
		SurveyObj.getObj(SurveyObj.sMapIdPre + i).style.width = SurveyObj.nPercents[i] + "%";
	}
	SurveyObj.getObj(SurveyObj.sPre+"0").innerHTML = Math.round(SurveyObj.nPercents[0]);
	SurveyObj.getObj(SurveyObj.sPre+"1").innerHTML = 100-Math.round(SurveyObj.nPercents[0]);
}
</script><!--[if !IE]>|xGv00|f41c23882b947a913686b9db79fcc5d7<![endif]-->
    </ul>
  </div>
</div>
<!--[if !IE]>|xGv00|9a5ed463581a4e6ae238939e717366df<![endif]--> 
        <!--热点调查结束--> 
