<script type="text/javascript">
        function gosearch(obj)
        {
                document.frmSearch.keyvalue.value=obj.innerHTML;
                document.frmSearch.submit();
        }
        
        
	function focusVerify(obj)
	{
		if(obj.value == obj.defaultValue)
		{
			obj.value="";
		}
		obj.style.color="#333";
	}
	
	function blurVerify(obj)
	{
		if((obj.value == obj.defaultValue) || (obj.value =="" ))
		{
			obj.value = obj.defaultValue;
			obj.style.color="#8B8C8C";
		}
	}
//搜索，在此仅一个简单搜索
	function search()
	{
		var wordlength = 30;
		var filterattr = "";
		var filtervalue = "";
		var filterstype = "";

		//先获取类别以及品牌的值以及输入的值
		var category = $("category").value;
		var manufacturer = $("manufacturer").value;
		var inputValue = $("simple_search").value;

		if (/^\s*$/.test(inputValue))
		{
			inputValue = "";
		}

		if(inputValue == $("simple_search").defaultValue)
		{
			inputValue = "";			
		}

		inputValue = inputValue.trim();

		if (category == "" && inputValue == "")
		{
			alert("请选择类别、品牌、输入搜索关键字");
			return false;						
		}
		
		//选择了类别+厂商
		if (category != "" && manufacturer != "")
		{
			filterattr = "26|7";	
			filtervalue = category + "|" + manufacturer;
			filterstype = "2|2";
		}else if (category != "")
		{
			//只选择了类别，因为只要选择了厂商则必须选择类别



			filterattr = "26";
			filtervalue = category;
			filterstype = "2";
		}
		
		//加上关键字F14






		if (inputValue.length > wordlength)
		{
			alert('关键字最多填写"' + wordlength + '"个汉字/字符!');
			$("simple_search").focus();
			return false;
		}
		
		//类别名称
		document.frmSearch.subcategory.value = category;
		document.frmSearch.filterattr.value = filterattr;
		document.frmSearch.filtervalue.value = filtervalue;
		document.frmSearch.filterstype.value = filterstype;
		document.frmSearch.keyvalue.value = inputValue;
		document.frmSearch.submit();
	}

//CategoryID与表中存储的FID做一个对应
function setManufacturerList(evt)
{
	var CategoryFID = evt.options[evt.selectedIndex].getAttribute("FID");
	//将选中的FID、DID赋值给form中






	document.frmSearch.subcategoryfid.value = CategoryFID;
	document.frmSearch.subcategoryid.value = evt.options[evt.selectedIndex].getAttribute("DID");;

	$("manufacturer").options.length = 1;
	$("manufacturer").options[0].selected;
	if(CategoryFID == "")
	{
		$("manufacturer").disabled = true;
		return;
	}

	$("manufacturer").disabled = false;


	JsLoader.load("http://data.tech.qq.com/subcategory/"+CategoryFID+"/cate_manu_json.js", function()
	{

		if (typeof oCateManuData != "undefined")
		{
			addManufacturerList(oCateManuData.arrCateManu);
		}
	});
}

function addManufacturerList(arrCateManu)
{
	var oOption = null;

	for(var i=0; i < arrCateManu.length - 1; i++)
	{
		oOption = document.createElement("OPTION");
		$("manufacturer").options.add(oOption);
		if(Browser.moz)
		{
			oOption.textContent = arrCateManu[i].Name;
		}
		else
		{
			oOption.innerText = arrCateManu[i].Name;
		}
		oOption.value = arrCateManu[i].ID;
	}

}
</script>

