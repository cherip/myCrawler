	sSid: 1651654,//调查ID号
	sformId: "form_survey",//提交投票所用的form表单的Id，一般可以不改。
	sPjtID: "PjtID",//form表单中提交调查ID号的input的name，不能修改。
	sRadioIdPre: "rad",//form表单中每个选项对象的Id号前缀。
