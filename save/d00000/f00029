<script type="text/javascript">
$(function(){			   
$(".item").hover(	
 function () { 
	$(this).find(".tier").show();
	$(this).find("a").eq(0).addClass("this_1");
  },
   function () {
	$(this).find(".tier").hide();
	$(this).find("a").eq(0).removeClass("this_1");
  }
  );

var ullen = $(".item").length;
$(".item").each(function(i){
	var dt = ullen-i;						   
   $(this).css("z-index",dt);
 });
});
</script>
