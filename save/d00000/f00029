    <script>
		var picArray = {};
		var linkArray = {};
			
				picArray[0] = "http://img8.zol-img.com/module/16_module_images/1451.jpg";
		linkArray[0] = "http://go.zol.com/topic/2605967.html";
				picArray[1] = "http://img7.zol-img.com/module/16_module_images/1442.jpg";
		linkArray[1] = "http://go.zol.com/topic/2613479.html";
				picArray[2] = "http://img4.zol-img.com/module/16_module_images/1441.jpg";
		linkArray[2] = "http://go.zol.com/topic/2614764.html";
				picArray[3] = "http://img5.zol-img.com/module/16_module_images/1439.jpg";
		linkArray[3] = "http://go.zol.com/topic/2617688.html";
			    	
		// 首页焦点图
		var currslid = 0;
		var slidint;	
		function setfoc(id){	
			document.getElementById("focpic").src  = picArray[id];
			document.getElementById("foclnk").href = linkArray[id];	
			currslid = id;
			for(i = 0;i < 4; i++){
				$("#tmb"+i).removeClass("current");										
			}	
			$("#tmb"+id).addClass("current");									
			stopit();
		}	
		
		function playnext(){				
			if (currslid == 3){
				currslid = 0;	
			}else {
				currslid++;	
			}				
			setfoc(currslid);
			playit();			
		}
				
		function playit(){
			slidint = setTimeout(playnext,3000);	
		}
				
		// 焦点图停止
		function stopit(){
			clearTimeout(slidint);
		}  
		
		// tab切换
		var show_time = 280;
		var time1 = "";
		var time2 = "";
		var table_time = "";
		var style_time = "";
		function tabs(parentId){	
			$('#'+parentId).children('li').mouseover(function(){
				var self = this;
				table_time = setTimeout(function(){
					var actClass = $(self).siblings('.current');	
					var rel_div = actClass.attr('rel');
					actClass.removeClass('current');	
					$('#'+rel_div).hide();					
					var now_div = $(self).attr('rel');	
					$(self).addClass('current');			
					var lazyarea = $('#'+now_div).children('textarea');
				    if(lazyarea.length == 1) {			
						lazyarea.hide();
						var lazyhtml = lazyarea.val();
						$('#'+now_div).html(lazyhtml);
					}			
					$('#'+now_div).show();						
				}, show_time);			
			});	
			$('#'+parentId).children('li').mouseout(function(){
				if (table_time){
					clearTimeout(table_time);		
				}		
			});
		}	
		
		function hotSell(parentId){	
			$('#'+parentId).children('em').mouseover(function(){
				var self = this;
				table_time = setTimeout(function(){
					var actClass = $(self).siblings('.act');	
					var rel_div = actClass.attr('rel');
					actClass.removeClass('act');	
					$('#'+rel_div).hide();					
					var now_div = $(self).attr('rel');	
					$(self).addClass('act');			
					var lazyarea = $('#'+now_div).children('textarea');
				    if(lazyarea.length == 1) {			
						lazyarea.hide();
						var lazyhtml = lazyarea.val();
						$('#'+now_div).html(lazyhtml);
					}			
					$('#'+now_div).show();						
				}, show_time);			
			});	
			$('#'+parentId).children('em').mouseout(function(){
				if (table_time){
					clearTimeout(table_time);		
				}		
			});
		}		
		// 今日热点
		function styleRemove(hasClassName, num, n){			
			for (var i = 1; i <= num; i++){
				$('#'+hasClassName + '_' + i) . removeClass("current");
				if (!$('#hot_img_' + i) . hasClass('none')){
					$('#hot_img_' + i) . addClass("none");				
				}				
			}
			$('#'+hasClassName + '_' + n) . addClass("current");
			$('#hot_img_' + n) . removeClass("none");				
		}		
		
		function overStyle(styleClass, num, n){
			for(var i = 1; i <= num; i++){
				$('#' + styleClass + '_' + i).removeClass("current");
			}			
			$('#' + styleClass + '_' + n).addClass('current');								
		}				  	
		$(document).ready(function(){
			$('#searchTypeId').val(0);
			playit() // 轮换图	
			tabs('good_nav'); 
			tabs('sale_nav');
			tabs('hot_rank');		
			tabs('sale_rank');	
			hotSell("hot_nav");
		//	ZS.Search.ChangeToSearchGoods();

			//动态商家
			var dong = window.setInterval(change_active,3000);
			function change_active(){		
				$(".warp_r").find("dl:visible:first").prev().slideDown(800,function(){
																					 $(".warp_r").find("dl:visible:last").hide();
																					 $(".warp_r").find("dl:visible:last").addClass("last_dd");
																					 }
																		);
				if($(".warp_r").find("dl:visible:last").nextAll().length>20){
					$(".warp_r").find("dl:visible:last").nextAll().replaceWith("");
				}
				if($(".warp_r").find("dl:visible:first").prevAll().length<10){
					$.get("http://www.zol.com/ajax/getDongtai.php", function(data){
											   $(".warp_r").find("dl:hidden:first").before(data);
					}); 			
				}
				
			}
		
			$(".swb_dl").live("mouseover", function(){
				clearInterval(dong);
			});
			$(".swb_dl").live("mouseout", function(){
				dong = window.setInterval(change_active,3000);
			});
			
			try {
			    document.execCommand('BackgroundImageCache', false, true);
			}catch(e){
			}			
												
		});
		// 子类列表	
		$('#category-list').find("table").mousemove(function(){
			var li_style = $(this).attr("class");
			if (li_style == "" || li_style == "last_table"){
				$(this).attr("class", "current");			
			}			
		});				
		$('#category-list').find("table").mouseout(function(){
			var li_style = $(this).attr("class");
			if (li_style == "current"){
				$(this).attr("class", "");			
			}			
		});		

		// 选择商品类型
		$('#searchType').mousemove(function(){
			$(this).children("li").show();
		});
		$('#searchType').mouseout(function(){
			$(this).children("li").hide();
			$('.the_1').show();
		});
		
		$('#searchType').children("li").click(function(){
				var liContent = $(this).attr("rel");				
				liContent = parseInt(liContent);
				if (liContent){
					$('#searchTypeContent').html("找商家");							
				}else {
					$('#searchTypeContent').html("找商品");						
				}
				getSearchForm(liContent);								
				$('#searchType').children("li").hide();
				$('.the_1').show();		
				$('#searchTypeId').val(liContent);		
		});
	 function getSearchForm(searchTypeId){
	 	var searchFromUrl = ZS.BaseUrl+"/ajax/search/getSearchForm.php?t="+Math.random();
	 	var searchKeyword = $('#keyword').val();
		$.get(
			searchFromUrl,
			{searchTypeId:searchTypeId, searchKeyword:searchKeyword},
			function(data){
				if (data){
					$('#searchFormContent').html(data);																
				}							
			}
		);	 		
	 }			
	 function SearchFrom(){	 			
		var keyword = $("#keyword").val();
		var searchTypeId = parseInt($("#searchTypeId").val());	
		var url = ZS.BaseUrl + "/ajax/list/getListUrl.php";
		$.getJSON(url,
			{callback:"CallDoSearch", keyword:keyword}				 
		);
		function CallDoSearch(data){
			var result = data.result;
			if(result){
				ZS.jumpUrl(result);
			}	
		}	 	 	
	 }	
	 var search_tips_ajax_url = ZS.BaseUrl + "/ajax/search/getSearchTips.php";		
	 var search_tips_url = ZS.BaseUrl + "/list.php?keyword=";	
	 var search_tips_autocomplete_url = ZS.JsBaseUrl + "/public/jquery.autocomplete.js";
	 var search_tips_css_url = ZS.CssBaseUrl + "/search/autocomplete.css";
	 var is_search_tips  = true;	 
	 function search_tips(form_name,search_keyword_name,search_tj_name){				
		 var default_result =  "error"; 			 						 
		 $("#" + search_keyword_name).autocomplete(search_tips_ajax_url, {
		 minChars: 3,
		 width: 312,
		 max: 10,
		 highlight: false,
		 scroll: false,
		 scrollHeight: 300,
		 selectFirst:false,
		 formatItem: function(data, i, n, value) {
				if(-1 < value.indexOf("error")){
					if(value.split(",")[1]){
						default_result = value.split(",")[1];
					}
					return default_result;
				}else{
					return value.split(",")[1];
					}
				},
				formatResult: function(data, value) {														
					if(-1 < value.indexOf("error")){								
						if(value.split(",")[1]){
							default_result = value.split(",")[1];
						}						
							return default_result;
					}else{					
						return value.split(",")[0];
					}
				},
				onclick_jump:true,
				form_name:form_name,
				search_tips_url:search_tips_url,
				delay: 100,
				addParams:{2:"str"}								
				});					
	 }		 
	 if(is_search_tips){		
			search_tips("search_form","keyword","do_search");
	 }			
var com_zol_stat_param = "100_101_0_0_0_0_0_0";
document.write(unescape("%3Cscript src='http://stat.zol.com/js/zolshopStat.js' type='text/javascript'%3E%3C/script%3E"));				 			
</script>  
