//var kv = '奔腾B50';
var kv = '';
function sfocus(){
	if(document.getElementById('k').value== kv){
	document.getElementById('k').value='';
	}
}
function sblur(){
	if(document.getElementById('k').value==''){
	document.getElementById('k').value=kv;
	}
}
document.getElementById('k').value=kv;
</script>
