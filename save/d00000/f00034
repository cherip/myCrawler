<script type="text/javascript">
	var bj_state = 'true';
	var sh_state = 'true';
	var gz_state = 'true';
	var bj_interval = null;
	var sh_interval = null;
	var gz_interval = null;

	var changeObj={'tab_name':'auto_show_tab_','page_name':'auto_show_page_','className':'nv active','page_num':'3'}
	
	function change_city(changeObj,page,city){
		var _city = city;
		var state = '';

		if(_city == 'bj' && bj_state == 'true'){
			state = 'bj';
			bj_state = 'false';
		}else if(_city == 'sh' && sh_state == 'true'){
			state = 'sh';
			sh_state = 'false';
		}else if(_city == 'gz' && gz_state == 'true'){
			state = 'gz';
			gz_state = 'false';
		}

		if(_city == state){
			for(var i=1;i<=changeObj.page_num;i++){
				if(i == page){
					document.getElementById(changeObj.page_name+i).style.display = 'block';
					document.getElementById('dealer_logo_'+_city).innerHTML= change_logo(_city);
					document.getElementById('focus_img_'+_city).innerHTML= focus_turn_js(_city);
				}else{
					document.getElementById(changeObj.page_name+i).style.display = 'none';
				}
			}
		}else{
			for(var i=1;i<=changeObj.page_num;i++){
				if(i == page){
					document.getElementById(changeObj.page_name+i).style.display = 'block';
				}else{
					document.getElementById(changeObj.page_name+i).style.display = 'none';
				}
			}
		}
	//新加的函数
		
	   _I('cheshi_content_div').className = "content_nt mar_top8";
	   $('#cheshi_div').hide();
	   $('#cheshi_price').hide();
	   showcitylist(2);
		if(city=='bj' && !bj_interval){
			//bj_interval = window.setInterval("show_price_auto_change('bj_auto_change_',2)",5000);
		}
		else if(city=='sh'  && !sh_interval){
			//sh_interval = window.setInterval("show_price_auto_change('sh_auto_change_',2)",5000);
		}
		else if(city=='gz'  && !gz_interval){
			gz_interval = window.setInterval("show_price_auto_change('gz_auto_change_',2)",5000);

		}
		change_bsg_list(city);
	//新加的函数




	}

	//经销商报价自动播放
	function show_price_auto_change(tab,page_num){
		
		if(page_num>1){

			for(var i=1;i<=page_num;i++){

				if(document.getElementById(tab+i).style.display == "block"){

					if(i != page_num-1){
						var j = i - 1;	
					}else{
						var j = 2;
					}

					document.getElementById(tab+i).style.display = "none";
					document.getElementById(tab+j).style.display = "block";

					break;
				}
			}
		}
	}



