<script src="js/jquery.pack.js" type="text/javascript"></script>
<script type="text/javascript">

document.domain = 'yule.com.cn'

$(document).ready(function(){	
// 幻灯片var t=false;
	var j=0;	
	$("#imgnav #btn").find("span").each(function(i){
		$("#imgnav #btn").find("span").eq(i).mouseover( 
			function(){
				var cur=$("#imgnav #img").find("img:visible").parent().prevAll().length;
				//alert(cur);
				if(i!==cur){
					j=i;
					$(this).addClass("hov").siblings().removeClass("hov");
					$("#imgnav #img").find("img:visible").fadeOut(200,
						function(){
							$("#imgnav #img").find("img").eq(i).fadeIn(300,function(){
								$("#imgnav #msg").find("li:visible").hide();
								$("#imgnav #msg").find("li").eq(i).show();					
							});			
						}
					);	
				}
			}		
		);
	});
	var $func_play=function(){			
		$("#imgnav #btn").find("span").eq(j).mouseover()
		j++;
		if(j==5){j=0;}		
	}
	t=setInterval($func_play,4000);
	$("#imgnav").hover(function(){
			clearInterval(t);
			//j=$("#img").find("img:visible").parent().prevAll().length+1;
			return j;
		},function(){
			t=setInterval($func_play,4000);
	});
//幻灯片结束
//电子公告牌开始
var jc=-1;
	$("#BulletinBoard #newslist li").hover(function(){
			$("#BulletinBoard #newslist li.focus").addClass("nofocus");
			$("#BulletinBoard #newslist li.focus").removeClass("focus");
			$(this).removeClass("nofocus");
			$(this).addClass("focus");
			},function(){
	});
//电子公告牌结束
//排行榜开始
	$("#Top").find(".snred").each(function(i){
		var obj = $(this);
		var temp = $(this).html();
		if (parseInt(temp)>3){
			obj.removeClass("snred");
		}
		if (temp.length==1){
			$(this).html("0" + $(this).html());
		}
	});
//排行榜结束
//调查开始
var supportNum, oppositionNum, newsVote, total;
newsVote = 0;
	newsVoteID = $("#newsVote #newsVoteDetail #VoteID").html();
	$.get("ajaxvote.asp", {Action:"get",ID:newsVoteID}, function (data, textStatus){
	
		
		this; 
		//alert(data);
		temp = data.split("|");
		support = (parseInt(temp[0])/parseInt(temp[2]))*100+35;
		opposition = (parseInt(temp[1])/parseInt(temp[2]))*100+35;
		supportNum = parseInt(temp[0]);
		oppositionNum = parseInt(temp[1]);
		total = parseInt(temp[2]);
		$("#newsVote #newsVoteDetail #SupportBar").css("width",support);
		$("#newsVote #newsVoteDetail #OppositionBar").css("width",opposition);
		$("#newsVote #newsVoteDetail #SupportBar").html(parseInt(temp[0]));
		$("#newsVote #newsVoteDetail #OppositionBar").html(parseInt(temp[1]));
		//alert(temp[1]);
	});
	
	
	
	$("#newsVote #newsVoteDetail #Support").click(function(){
		if(newsVote == 0) {
			newsVoteID = $("#newsVote  #newsVoteDetail #VoteID").html()
;
			$.get("ajaxvote.asp", {Action:"voteSave",ID:newsVoteID,voteOpt:"0"}, function (data, textStatus){
				total = total + 1;
				supportNum = supportNum +1;
				newsVote = 1;
				$("#newsVote #newsVoteDetail #SupportBar").css("width",supportNum/total*100+35);
				$("#newsVote #newsVoteDetail #OppositionBar").css("width",oppositionNum/total*100+35);
				$("#newsVote #newsVoteDetail #SupportBar").html(parseInt(supportNum));
				$("#newsVote #newsVoteDetail #OppositionBar").html(parseInt(oppositionNum));
			});
		} else {
			alert("您已投过票了。")
		}
	});

	$("#newsVote #newsVoteDetail #Opposition").click(function(){
		if(newsVote == 0) {
			newsVoteID = $("#newsVote  #newsVoteDetail #VoteID").html()
;
			$.get("ajaxvote.asp", {Action:"voteSave",ID:newsVoteID,voteOpt:"1"}, function (data, textStatus){
				total = total + 1;
				oppositionNum = oppositionNum +1;
				newsVote = 1;
				$("#newsVote #newsVoteDetail #SupportBar").css("width",supportNum/total*100+35);
				$("#newsVote #newsVoteDetail #OppositionBar").css("width",oppositionNum/total*100+35);
				$("#newsVote #newsVoteDetail #SupportBar").html(parseInt(supportNum));
				$("#newsVote #newsVoteDetail #OppositionBar").html(parseInt(oppositionNum));
			});
		} else {
			alert("您已投过票了。")
		}
	});
//调查结束

//临时 周杰伦新专辑《惊叹号》？调查开始
var Temp_zjl_supportNum, Temp_zjl_newsVote, Temp_zjl_newsVoteID;
Temp_zjl_newsVote = 0;
	Temp_zjl_newsVoteID = 17;
	$.get("ajaxvote.asp", {Action:"get",ID:Temp_zjl_newsVoteID}, function (data, textStatus){
		this; 
		//alert(data);
		temp = data.split("|");
		Temp_zjl_supportNum = parseInt(temp[0]);
		$("#bdAd #FCNum").html(parseInt(temp[0]));
		//alert(temp[0]);
	});
	
	
	
	$("#bdAd #FCBtn").click(function(){
		if(newsVote == 0) {
			Temp_zjl_newsVoteID = 17;
;
			$.get("ajaxvote.asp", {Action:"voteSave",ID:Temp_zjl_newsVoteID,voteOpt:"0"}, function (data, textStatus){
				total = total + 1;
				Temp_zjl_supportNum = Temp_zjl_supportNum +1;
				newsVote = 1;
				$("#bdAd #FCNum").html(parseInt(Temp_zjl_supportNum));
			});
		} else {
			alert("您已投过票了。")
		}
	});

//临时 周杰伦新专辑《惊叹号》？调查结束


//音乐图片列表开始
	$("#RecommendedSongs li").hover(function(){
		$("#RecommendedSongsTemp").html($(this).html());
		$("#RecommendedSongsTemp").css({"position":"absolute", "padding-top":"4px", "z-index":"100", "width":"132px", "height":"74px", "background":"#CC0000 url(images/RecommendedSongsTemp.gif) left top repeat-x", "text-align":"center", "display":"none", "overflow":"visible", "line-height":"20px"});
		$("#RecommendedSongsTemp").css("top", $(this).offset().top);
		$("#RecommendedSongsTemp").css("left", $(this).offset().left);
		$("#RecommendedSongsTemp").stop();
		$("#RecommendedSongsTemp").show("fast");
		$("#RecommendedSongsTemp").animate({"opacity": "true","height": 95},600,function(){});
	},function(){
	});
	$("#RecommendedSongsTemp").hover(function(){
	},function(){
		$("#RecommendedSongsTemp").stop();
		$("#RecommendedSongsTemp").animate({"height": 75},600,function(){$("#RecommendedSongsTemp").hide("fast");});
	});
//音乐图片列表结束
//搜索框开始
	var searchTextTempStr = "";
	$(".SearchText").hover(function(){
		if($(this).val().substr(0,3)=="请输入") {
			searchTextTempStr = $(this).val();
			$(this).val("");
		}
	},function(){
		if($(this).val()==""||$(this).val().substr(0,3)=="请输入") {
			$(this).val(searchTextTempStr);
			searchTextTempStr = "";
		}
	});

//搜索框结束
//影评与观影团开始
	$("#Critics").css("display", "none");
	$("#ViewingGroup #FCBlack").hover(function(){
		$("#ViewingGroup").css("display", "none");
		$("#Critics").css("display", "");
	},function(){
	});
	$("#Critics #FCBlack").hover(function(){
		$("#ViewingGroup").css("display", "");
		$("#Critics").css("display", "none");
	},function(){
	});
//影评与观影团结束

//高清视频开始
	$("#v_movie.tx").hover(function(){
		if($(this).val()=="方大同") {
			searchTextTempStr = $(this).val();
			$(this).val("");
		}
	}, function(){
		if($(this).val()=="") {
			$(this).val("方大同");
		}

	});

	$("#HighDefinitionLeftContent .HDLC_2").css("display", "none");
	$("#HighDefinitionLeftContent .HDLC_3").css("display", "none");
	$("#HighDefinitionLeftContent .HDLC_li_1").hover(function(){
		$("#HighDefinitionLeftContent .HDLC_1").css("display", "");
		$("#HighDefinitionLeftContent .HDLC_2").css("display", "none");
		$("#HighDefinitionLeftContent .HDLC_3").css("display", "none");
	},function(){
	});
	$("#HighDefinitionLeftContent .HDLC_li_2").hover(function(){
		$("#HighDefinitionLeftContent .HDLC_1").css("display", "none");
		$("#HighDefinitionLeftContent .HDLC_2").css("display", "");
		$("#HighDefinitionLeftContent .HDLC_3").css("display", "none");
	},function(){
	});
	$("#HighDefinitionLeftContent .HDLC_li_3").hover(function(){
		$("#HighDefinitionLeftContent .HDLC_1").css("display", "none");
		$("#HighDefinitionLeftContent .HDLC_2").css("display", "none");
		$("#HighDefinitionLeftContent .HDLC_3").css("display", "");
	},function(){
	});
//高清视频结束

});
